{
  "name": "glitch-hello-react",
  "version": "0.0.3",
  "description": "A simple React single page app, built with Vite and React.",
  "scripts": {
    "start": "WSL_DISTRO_NAME=foo firebase emulators:exec --ui --project demo-app 'npm run start:all'",
    "start:all": "concurrently \"npm:start:client\" \"npm:start:server\"",
    "start:server": "node backend/server",
    "start:client": "echo foo && vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-spring": "^9.4.3",
    "wouter": "^2.7.4",
    "react-helmet-async": "^1.2.3",
    "express": "^4.17.3",
    "express-http-proxy": "^1.6.3",
    "concurrently": "^7.0.0",
    "wait-on": "^6.0.1",
    "http-proxy-middleware": "^2.0.3",
    "firebase-tools": "^10.2.1",
    "firebase": "^9.6.8",
    "reactfire": "^4.2.1",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "vite": "^2.4.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/glitch-hello-react"
  },
  "glitch": {
    "projectType": "generated_static"
  }
}
